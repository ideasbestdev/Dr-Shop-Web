{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": false,
        "displayName": false,
        "fileName": false,
        "meaninglessFileNames": ["index", "styles"],
        "minify": true,
        "pure": true,
        "transpileTemplateLiterals": false
      }
    ]
  ]
}